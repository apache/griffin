{
  "name": "accu1",
  "type": "accuracy",

  "source": {
    "type": "hive",
    "version": "1.2",
    "config": {
      "database": "default",
      "table.name": "users_info_src",
      "partitions": "dt=23123, hour=432; dt=35464, hour=4657"
    }
  },

  "target": {
    "type": "hive",
    "version": "1.2",
    "config": {
      "database": "default",
      "table.name": "users_info_target",
      "partitions": "dt=23123, hour=432; dt=35464, hour=4657"
    }
  },

  "evaluateRule": {
    "sampleRatio": 0.2,
    "rules": "@Key ${source}['user_id'] === ${target}['user_id']; ${source}['first_name'] === ${target}['first_name']; ${source}['last_name'] === ${target}['last_name']; ${source}['address'] === ${target}['address']; ${source}['email'] === ${target}['email']; ${source}['phone'] === ${target}['phone']; ${source}['post_code'] === ${target}['post_code']"
  }
}