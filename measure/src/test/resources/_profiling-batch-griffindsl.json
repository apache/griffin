{
  "name": "prof_batch",

  "process.type": "batch",

  "timestamp": 123456,

  "data.sources": [
    {
      "name": "source",
      "connectors": [
        {
          "type": "avro",
          "version": "1.7",
          "config": {
            "file.name": "src/test/resources/users_info_src.avro"
          },
          "pre.proc": [
            {
              "dsl.type": "spark-sql",
              "name": "${this}",
              "rule": "select reg_replace(email, '^([^@0-9]+)([0-9]+)@(dc)(?:\\\\.[^@]+)$', '$1@$3') as email, post_code from ${this}"
            }
          ]
        }
      ]
    }
  ],

  "evaluate.rule": {
    "rules": [
      {
        "dsl.type": "griffin-dsl",
        "dq.type": "profiling",
        "name": "prof",
        "rule": "email, count(*) from source group by email",
        "metric": {
          "name": "prof",
          "collect.type": "array"
        }
      },
      {
        "dsl.type": "griffin-dsl",
        "dq.type": "profiling",
        "name": "grp",
        "rule": "source.post_code, count(*) from source group by source.post_code",
        "metric": {
          "name": "post_group",
          "collect.type": "array"
        }
      }
    ]
  }
}