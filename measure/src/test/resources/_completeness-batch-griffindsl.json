{
  "name": "comp_batch",

  "process.type": "batch",

  "timestamp": 123456,

  "data.sources": [
    {
      "name": "source",
      "connectors": [
        {
          "type": "avro",
          "version": "1.7",
          "config": {
            "file.name": "src/test/resources/users_info_src.avro"
          }
        }
      ]
    }
  ],

  "evaluate.rule": {
    "rules": [
      {
        "dsl.type": "griffin-dsl",
        "dq.type": "completeness",
        "out.dataframe.name": "comp",
        "rule": "email, post_code, first_name",
        "out":[
          {
            "type": "metric",
            "name": "comp"
          }
        ]
      }
    ]
  },

  "sinks": ["CONSOLE","ELASTICSEARCH"]
}