{
  "name": "dup_batch",

  "process.type": "batch",

  "data.sources": [
    {
      "name": "source",
      "baseline": true,
      "connectors": [
        {
          "type": "avro",
          "version": "1.7",
          "config": {
            "file.name": "src/test/resources/users_info_src.avro"
          }
        }
      ]
    },
    {
      "name": "target",
      "connectors": [
        {
          "type": "avro",
          "version": "1.7",
          "config": {
            "file.name": "src/test/resources/users_info_src.avro"
          }
        }
      ]
    }
  ],

  "evaluate.rule": {
    "rules": [
      {
        "dsl.type": "griffin-dsl",
        "dq.type": "duplicate",
        "name": "dup",
        "rule": "user_id",
        "details": {
          "source": "source",
          "target": "target",
          "dup": "dup",
          "num": "num"
        },
        "metric": {
          "name": "dup"
        },
        "record": {
          "name": "dupRecords"
        }
      }
    ]
  }
}