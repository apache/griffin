{
  "name": "prof_batch_test",

  "process.type": "batch",

  "data.sources": [
    {
      "name": "source",
      "connectors": [
        {
          "type": "avro",
          "version": "1.7",
          "config": {
            "file.name": "src/test/resources/users_info_src.avro"
          }
        }
      ]
    }
  ],

  "evaluateRule": {
    "rules": [
      {
        "dsl.type": "griffin-dsl",
        "dq.type": "profiling",
        "rule": "source.user_id.max(), source.first_name.count() as cnt",
        "details": {
          "source": "source",
          "profiling": {
            "name": "prof",
            "persist.type": "metric"
          }
        }
      }
    ]
  }
}